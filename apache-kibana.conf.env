<VirtualHost _default_:80>
  DocumentRoot /var/www

  <Directory /var/www>
    Options Indexes FollowSymLinks MultiViews
    Options All
    AllowOverride All
    Require all granted

    # RewriteEngine on
  </Directory>

  ProxyPass /elasticsearch http://${ELASTICSEARCH_1_PORT_9200_TCP_ADDR}:${ELASTICSEARCH_1_PORT_9200_TCP_PORT}
  ProxyPassReverse /elasticsearch http://${ELASTICSEARCH_1_PORT_9200_TCP_ADDR}:${ELASTICSEARCH_1_PORT_9200_TCP_PORT}

  # ErrorLog "|more"
  ErrorLog ${APACHE_LOG_DIR}/error.log
  #CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost>
